public class Practical9 {

    public static void main(String[] args) {
        drawBoxedGrid();
    }

    public static void drawBoxedGrid() {
        String horizontal = "+---+---+---+";
        String emptyRow = "|   |   |   |";

        System.out.println("3x3 Boxed Grid:\n");

        for (int i = 0; i < 3; i++) {
            System.out.println(horizontal);
            System.out.println(emptyRow);
        }
        System.out.println(horizontal);
    }
}
